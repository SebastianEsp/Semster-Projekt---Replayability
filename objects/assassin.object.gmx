            <string>var ID;
ID = id;

if keyboard_check(ord(Up)){
  hit_dir = "up";
}
else{
  if keyboard_check(ord(Right)){
    hit_dir = "right";
  }
  if keyboard_check(ord(Left)){
   hit_dir = "left";
  }
}
//Normal hit
if keyboard_check_pressed(ord(A1)) &amp;&amp; canhit = true
{
audio_play_sound(choose(katana_1_sound,katana_2_sound),0,0);
//Normal hit up
if hit_dir = "up"{
  with instance_create(x,y-32,fist_obj){
    owner = ID;
    dir = "up";
  }
}
//Normal hit left
if hit_dir = "left"{
  with instance_create(x-32,y,fist_obj){
    owner = ID;
    dir = "left";
//    motion_add(150,10);
    y-=1;
  }
}
//Normal hit right
if hit_dir = "right"{
  with instance_create(x+32,y,fist_obj){
    owner = ID;
    dir = "right";
  }
}
//Hit cooldown
canhit = false;
ishitting = true;
katana_weapon.visible = false;
alarm[0] = 10;
alarm[6] = 10;

}
//Power hit
if keyboard_check_pressed(ord(A2)) &amp;&amp; canPowerHit = true &amp;&amp; canCast = true
{
katana_weapon.visible = false;
if hit_dir = "up"{
  with instance_create(x,y-16,fist_obj){
    owner = ID;
    dir = "up";
    assassin.vspeed -=6
    jumps=1;
  }
}

if hit_dir = "left"{
  with instance_create(x-16,y,fist_obj){
    owner = ID;
    dir = "left";
    motion_add(150,10);
    y-=1;
    assassin.hspeed -=15
    assassin.vspeed -=1
    assassin.canMove = false;
    jumps=1;
  }
}

if hit_dir = "right"{
  with instance_create(x+16,y,fist_obj){
    owner = ID;
    dir = "right";
    assassin.hspeed +=15
    assassin.vspeed -=1
    assassin.canMove = false;
    jumps=1;
  }
}
//Powerhit cooldown
canPowerHit = false;
alarm[2] = 60;
//Bremser spiller
alarm[3] = 10;
//Bevægelse efter slag muligt
alarm[4] = 12
}


