            <string>var ID;
ID = id;

if keyboard_check(ord(Up)){
  hit_dir = "up";
}
else{
  if keyboard_check(ord(Right)){
    hit_dir = "right";
  }
  if keyboard_check(ord(Left)){
   hit_dir = "left";
  }
}
//Normal hit
if keyboard_check(ord(A1)) &amp;&amp; canshoot = true
{
//Shoot up
if hit_dir = "up"{
  with instance_create(x+15,y-40,missile_obj){
    owner = ID;
    direction = 90;
    image_angle = 90;
  }
}
//Shoot left
if hit_dir = "left"{
  with instance_create(x-31,y-22,missile_obj){
    owner = ID;
    direction = 180;
    image_angle = 182;
    y-=1;
  }
}
//Shoot right
if hit_dir = "right"{
  with instance_create(x+28,y-22,missile_obj){
    owner = ID;
    direction = 0;
    image_angle = -2;
  }
}
audio_play_sound(magic_missile_sound,0,0);
//Hit cooldown
canshoot = false;
if empowered = true
{
    alarm[0] = 10;
}
else
{
    alarm[0] = 40;
}
isshooting = true;
alarm[6] = 20;
}



